<FindBugsFilter>
    <Match>
        <Class name="org.komamitsu.fluency.fluentd.ingester.sender.RequestOption"/>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="org.komamitsu.fluency.fluentd.ingester.Response"/>
        <Bug pattern="EI_EXPOSE_REP, EI_EXPOSE_REP2"/>
    </Match>
    <Match>
        <Class name="org.komamitsu.fluency.aws.s3.recordformat.MessagePackRecordFormatter"/>
        <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS"/>
    </Match>
    <Match>
        <Class name="org.komamitsu.fluency.aws.s3.recordformat.MessagePackRecordFormatter$Config"/>
        <Bug pattern="NM_SAME_SIMPLE_NAME_AS_SUPERCLASS"/>
    </Match>
    <Match>
        <Bug pattern="CNT_ROUGH_CONSTANT_VALUE"/>
    </Match>
    <Match>
        <!-- RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE is from a bug on JDK11? -->
        <Bug pattern="RCN_REDUNDANT_NULLCHECK_WOULD_HAVE_BEEN_A_NPE"/>
    </Match>
</FindBugsFilter>
